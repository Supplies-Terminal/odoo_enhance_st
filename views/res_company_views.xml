<?xml version="1.0"?>
<odoo>
    <record id="view_company_form_inherit_st" model="ir.ui.view">
        <field name="name">res.company.form.st.inherit</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="	res.company.form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='social_media']" position="after">
                <group name="service">
                    <field name="service_def"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
